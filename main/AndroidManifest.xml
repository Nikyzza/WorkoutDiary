<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.edzesnaplo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Edzesnaplo">
        <activity android:name=".WorkoutsListActivity"
            android:exported="false"/>
        <activity android:name=".ProfileSettingsActivity"
            android:exported="false"/>
        <activity android:name=".sports.AerobicActivity"
            android:exported="false"/>
        <activity android:name=".sports.ClimbingActivity"
            android:exported="false"/>
        <activity android:name=".sports.CyclingActivity"
            android:exported="false"/>
        <activity android:name=".sports.FootballActivity"
            android:exported="false"/>
        <activity android:name=".sports.KayakingActivity"
            android:exported="false"/>
        <activity android:name=".sports.LiftingActivity"
            android:exported="false"/>
        <activity android:name=".sports.RunActivity"
            android:exported="false"/>
        <activity android:name=".sports.SwimmingActivity"
            android:exported="false"/>
        <activity android:name=".RegistrationActivity"
            android:exported="false"/>
        <activity android:name=".TrainingActivity"
            android:exported="false"/>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>